<robot name="azi_bot">
	<link name="box">
		<visual>
			<geometry>
				<box size = "1 0.4 0.2"/>
			</geometry>
			<origin xyz="0.5 0.2 0.1" rpy="0 0 0"/>
			<material name="yellow">
				<color rgba="0.8 0.8 0 1"/>
			</material>
		</visual>
	</link>s

	<link name="wheel_1">
		<visual>
			<geometry>
				<cylinder length="0.05" radius="0.1"/>
			</geometry>
			<origin xyz="0 0 0" rpy="1.57 0 0"/>
			<material name="black">
				<color rgba="0 0 0 1"/>
			</material>
		</visual>
		
	</link>

	<link name="wheel_2">
		<visual>
			<geometry>
				<cylinder length="0.05" radius="0.1"/>
			</geometry>
			<origin xyz="0 0 0" rpy="1.57 0 0"/>
			<material name="black"/>
		</visual>
	</link>

	<link name="wheel_3">
		<visual>
			<geometry>
				<cylinder length="0.05" radius="0.1"/>
			</geometry>
			<origin xyz="0 0 0" rpy="1.57 0 0"/>
			<material name="black"/>		
		</visual>
	</link>

	<link name="wheel_4">
		<visual>
			<geometry>
				<cylinder length="0.05" radius="0.1"/>
			</geometry>
			<origin xyz="0 0 0" rpy="1.57 0 0"/>
			<material name="black"/>	
		</visual>
	</link>

	<link name="neck">
		<visual>
			<geometry>
				<cylinder length="0.2" radius="0.05"/>
			</geometry>
			<origin xyz="0.025 0 0.096" rpy="0 0 0"/>
		</visual>
	</link>

	<link name="camera">
		<visual>
			<geometry>
				<box size = "0.4 0.2 0.2"/>
			</geometry>
			<origin xyz="0 0 0.1" rpy="0 0 0"/>
		</visual>
	</link>

	<link name="gripper">
		<visual>
			<geometry>
				<!-- <box size = "0.1 0.1 0.1"/> -->
				<mesh filename="package://pr2_description/meshes/gripper_v0/l_finger.dae"/>
			</geometry>
			<origin xyz="0.1 0 0" rpy="1.57 0 0"/>
		</visual>
	</link>

	<link name="end_effector">
		<visual>
			<geometry>
				<mesh filename="package://pr2_description/meshes/gripper_v0/l_finger.dae"/>
			</geometry>
			<origin xyz="0 0 0" rpy="1.57 0 0"/>
		</visual>
	</link>



<!-- joints -->
	<joint name="wh1_joint" type="continuous">
		<parent link="box"/>
		<child link="wheel_1"/>
		<origin xyz="0.2 -0.05 0" rpy="0 6.28 0"/>
		<axis xyz=" 0 1 0"/>
	</joint>

	<joint name="wh2_joint" type="continuous">
		<parent link="box"/>
		<child link="wheel_2"/>
		<origin xyz="0.8 -0.05 0" rpy="0 6.28 0"/>
		<axis xyz=" 0 1 0"/>
	</joint>

	<joint name="wh3_joint" type="continuous">
		<parent link="box"/>
		<child link="wheel_3"/>
		<origin xyz="0.2 0.45 0" rpy="0 6.28 0"/>
		<axis xyz=" 0 1 0" rpy="0 0 0"/>
	</joint>

	<joint name="wh4_joint" type="continuous">
		<parent link="box"/>
		<child link="wheel_4"/>
		<origin xyz="0.8 0.45 0" rpy="0 6.28 0"/>
		<axis xyz=" 0 1 0"/>
	</joint>

	<joint name="neck_joint" type="fixed">
		<parent link="box"/>
		<child link="neck"/>
		<origin xyz="0.2 0.2 0.2" rpy="0 0 0"/>
		<axis xyz=" 0 0 0"/>
	</joint>

	<joint name="camera_joint" type="revolute">
		<parent link="neck"/>
		<child link="camera"/>
		<origin xyz="0 0 0.2" rpy="0 0 4.71"/>
		<axis xyz=" 0 0 1"/>
		<limit effort="1000.0" lower="-2.36" upper="2.36" velocity="0.5"/>
	</joint>

	<joint name="gripper_joint" type="continuous">
		<parent link="box"/>
		<child link="gripper"/>
		<origin xyz="0.9 0.2 0.2" rpy="0 0 0"/>
		<!-- <limit effort="1000.0" lower="-2.36" upper="2.36" velocity="0.5"/> -->
	</joint>

	<joint name="end_joint" type="revolute">
		<parent link="gripper"/>
		<child link="end_effector"/>
		<origin xyz="0.2 0 0" rpy="0 0 0"/>
		<limit effort="1000.0" lower="-2.36" upper="2.36" velocity="0.5"/>
	</joint>
</robot>
